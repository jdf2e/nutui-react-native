{
  "name": "@nutui/nutui-react-native",
  "version": "0.0.8",
  "scripts": {
    "bootstrap": "yarn --cwd demo && yarn",
    "dev:ios": "yarn --cwd demo ios",
    "dev": "yarn --cwd demo web",
    "doc": "npm run generate:docs && vite --open --force",
    "doc:jd": "npm run generate:jd:docs && vite --open --force",
    "generate:docs": "node scripts/generate-nutui.js",
    "generate:jd:docs": "node scripts/generate-jd-nutui.js",
    "build:docs": "npm run generate:docs && vite build --base=/react-native/",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "preview": "vite preview",
    "prepack": "bob build",
    "test": "jest --passWithNoTests",
    "typecheck": "npm run generate:docs && tsc --noEmit",
    "release": "release-it --ci --verbose"
  },
  "author": "jdf2e",
  "license": "MIT",
  "repository": "https://github.com/jdf2e/nutui-react-native",
  "bugs": {
    "url": "https://github.com/jdf2e/nutui-react-native/issues"
  },
  "homepage": "https://github.com/jdf2e/nutui-react-native#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@bem-react/classname": "^1.5.12",
    "@react-spring/native": "9.3.2",
    "react-native-root-portal": "^1.0.9"
  },
  "peerDependencies": {
    "expo-linear-gradient": "^12.0.1",
    "react": "*",
    "react-native": "*",
    "react-native-linear-gradient": "^2.6.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.2.2",
    "@react-native-community/eslint-config": "^3.0.2",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^28.1.2",
    "@types/node": "^18.11.9",
    "@types/postcss-import": "^14.0.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-native": "^0.70.8",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "commitlint": "^17.0.2",
    "dedent": "^0.7.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "expo-modules-core": "^1.1.1",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "jest": "^28.1.1",
    "lzutf8": "^0.6.2",
    "markdown-navbar": "^1.4.3",
    "pod-install": "^0.1.0",
    "postcss-import": "^15.0.0",
    "prettier": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-native": "^0.72.4",
    "react-native-builder-bob": "^0.20.3",
    "react-router-dom": "^6.12.0",
    "react-syntax-highlighter": "^15.4.5",
    "rehype-raw": "^6.1.1",
    "release-it": "^15.0.0",
    "remark-directive": "^2.0.1",
    "remark-gfm": "^3.0.1",
    "sass": "^1.56.0",
    "typescript": "^4.6.4",
    "unist-builder": "^3.0.1",
    "vite": "^3.2.3"
  },
  "main": "lib/commonjs/index.js",
  "react-native": "components/index.ts",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "source": "components",
  "files": [
    "components",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "react-native-builder-bob": {
    "source": "components",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "nutui"
  ],
  "engines": {
    "node": ">= 16.0.0"
  },
  "packageManager": "^yarn@1.22.15",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "header": "# Changelog",
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "components/",
    "demo/",
    "sites/",
    "vite/",
    "scripts/",
    "dist/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
